{
    "name": "sintret/yii2-advanced",
    "description": "Yii 2 Advanced Template sintret ",
    "keywords": ["yii2", "framework", "advanced", "application template","adminlte","chat","todolist","whatsapp"],
    "homepage": "http://sintret.com/",
    "type": "project",
    "license": "BSD-3-Clause",
     "support": {
        "issues": "https://github.com/sintret/yii2-advanced/issues",
        "forum": "http://sintret.com/blog_detail/523/yii2_create_project_with_adminlte_basic_and_advanced_easyly",
        "source": "https://github.com/sintret/yii2-advanced"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "kartik-v/yii2-detail-view": "*",
        "kartik-v/yii2-password": "dev-master",
        "sendgrid/sendgrid": "2.1.1",
        "miloschuman/yii2-highcharts-widget": "*",
        "mihaildev/yii2-ckeditor": "*",
        "sintret/yii2-gii-adminlte": "dev-master",
        "sintret/yii2-chat-adminlte": "dev-master",
        "sintret/yii2-todolist": "dev-master",
        "sintret/whatsapp": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
